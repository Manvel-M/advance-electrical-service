---
import { Header } from "@/components/sections";
import "@/styles/app.css";

type Props = {
  title: string;
};

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title title={title}></title>
  </head>
  <body>
    <Header />
    <slot />
  </body>
</html>
